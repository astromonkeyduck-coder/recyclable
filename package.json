{
  "name": "isthisrecyclable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "extract:orlando": "tsx scripts/extract-provider-orlando.ts",
    "generate:og": "tsx scripts/generate-og-fallbacks.ts",
    "cap:sync": "npx cap sync",
    "cap:ios": "npx cap open ios",
    "cap:android": "npx cap open android",
    "cap:dev": "CAPACITOR_ENV=development CAPACITOR_DEV_IP=$(node -e \"const os=require('os');const i=Object.values(os.networkInterfaces()).flat().find(x=>!x.internal&&x.family==='IPv4');console.log(i?.address||'localhost')\") npx cap sync && npx cap open ios",
    "cap:dev:android": "CAPACITOR_ENV=development CAPACITOR_DEV_IP=$(node -e \"const os=require('os');const i=Object.values(os.networkInterfaces()).flat().find(x=>!x.internal&&x.family==='IPv4');console.log(i?.address||'localhost')\") npx cap sync && npx cap open android",
    "cap:build": "CAPACITOR_ENV=production npx cap sync"
  },
  "dependencies": {
    "@capacitor/camera": "^8.0.1",
    "@capacitor/core": "^8.1.0",
    "@tanstack/react-query": "^5.90.21",
    "cheerio": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.3",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "openai": "^6.25.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/ios": "^8.1.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.8.1",
    "shadcn": "^3.8.5",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
